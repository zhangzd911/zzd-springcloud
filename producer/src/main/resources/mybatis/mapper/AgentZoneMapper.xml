<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzd.mapper.AgentZoneMapper">

    <select id="byid" parameterType="java.lang.String" resultType="com.zzd.entity.AgentZone">
        SELECT * FROM  agent_zone WHERE id = #{id, jdbcType=VARCHAR}
    </select>

    <select id="list"  resultType="com.zzd.entity.AgentZone">
        SELECT * FROM  agent_zone GROUP BY id
    </select>

    <select id="byMapperOtherEntity" parameterType="java.lang.String" resultType="com.zzd.dto.AgentZoneDto">
        SELECT id,zone_code,agentid FROM  agent_zone WHERE agentid = 279
    </select>
</mapper>